{
  "name": "full-stack-react-demo",
  "version": "1.0.0",
  "description": "full stack react demo",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm install -g cross-env @babel/cli ",
    "build": "webpack -p",
    "deploy": "npm run build && git commit -a -m deployment && git push heroku master",
    "dev": "webpack-dev-server --open",
    "server": "cross-env NODE_ENV=production babel-node src/server",
    "server-dev": "nodemon src/server --exec babel-node src/server",
    "start-dev": "concurrently \"npm run server-dev\" \"npm run dev\"",
    "server-spec": "babel-node src/server/server.spec",
    "start": "npm run server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "nodemon": "^1.19.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "history": "^4.9.0",
    "md5": "^2.2.1",
    "mongodb": "^3.2.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.2",
    "uuid": "^3.3.2"
  }
}
